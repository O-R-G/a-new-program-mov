				<dict>
					<key>COMMAND_STRING</key>
					<string>#!/bin/bash

# adjust speed of quicktime player
# only in range 0.1 - 1.9x so that audio scrubs
# trigger via hotkey for automator service

# read _tmp.txt
IN='/Users/reinfurt/Library/Services/__tmp.txt'
SCRIPT='/Users/reinfurt/Library/Services/__run.scpt'
SPEED=`cat $IN`
INCREMENT=0.1

# increment
if [[ "$SPEED" != "0.1" ]]; then
    SPEED=$(echo "$SPEED - $INCREMENT" | bc)
fi

# adjust quicktime speed, pass argument
osascript $SCRIPT $SPEED

# write _tmp.txt
echo $SPEED &gt; $IN

# cleanup
echo $SPEED
echo "done."

exit</string>
